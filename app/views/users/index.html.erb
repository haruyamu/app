<div class="my_profile">
  <div class="upper">
    <div class="left">
      User page
    </div>
    <div class=" right">
      <div class="right_edit">
        <%= link_to  "user edit", edit_user_path(current_user.id) %>
      </div>
      <div class="right_edit">
        <%= link_to  "post edit", "#" %>
      </div>
    </div>
  </div>

  <div class="lower">
    <%= @posts.each do |p| %>
    <div class="col-md-6 mx-auto mt-5">
      <div class="card flex-md-row mb-4 shadow-sm h-md-250">
        <div class="card-body p_height">
          <div class="d-inline-block mb-2 text-success"><%=p.place%></div>
          <h3 class="mb-0">
            <p class="title_plus"><%= p.title %></p>
          </h3>
          <h3 class="mb-0">
            <p class="content_plus"><%= p.content %></p>
          </h3>
          <%= link_to p.user.name + "さん",users_path, class:" mb-2 text-primary " %>
        </div>
        <%= image_tag p.image, class: "card-img-right flex-auto d-none d-lg-block pic-size" if p.image.attached?%>
      </div>
    </div>
    <%end%>
  </div>